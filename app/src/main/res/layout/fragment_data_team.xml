<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >


    <TextView
        android:id="@+id/tv_seoul_team_information"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:text="구단 정보"
        android:textSize="20sp"
        android:fontFamily="@font/font_nanumsquare_bold"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <Button
        android:id="@+id/btn_seoul_team_back"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/baseline_arrow_back_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="332dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/ic_star_top"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/ic_fc_seoul"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="260dp"
        android:layout_height="70dp"
        android:layout_marginTop="92dp"
        android:layout_marginStart="105dp"
        android:src="@drawable/bg_slide_bar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_seoul_team_real"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:text="FC 서울"
        android:textSize="17sp"
        android:fontFamily="@font/font_nanumsquare_bold"
        android:layout_marginStart="25dp"
        android:layout_marginTop="150dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

    <TextView
        android:id="@+id/seoul_team_match_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="109dp"
        android:layout_marginLeft="125dp"
        android:text="경기 진행 중"
        android:textSize="9sp"
        android:fontFamily="@font/font_nanumsquare_bold"
        android:textColor="#9D0000"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="@id/tv_seoul_team_match_time"
        android:layout_marginRight="2dp"

        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="107dp"
        android:text="축구 / K-리그"
        android:textSize="12sp"
        android:fontFamily="@font/font_nanumsquare_regular"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@id/seoul_team_match_with"

        />

    <TextView
        android:id="@+id/seoul_team_match_with"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="127dp"
        android:text="FC 서울 vs 전북 현대"
        android:fontFamily="@font/font_nanumsquare_bold"
        android:layout_marginStart="183dp"
        android:textSize="18sp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:id="@+id/tv_seoul_team_match_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="127dp"
        android:layout_marginStart="127dp"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:text="19:00"
        android:fontFamily="@font/font_nanumsquare_bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        />

    <TextView
        android:id="@+id/tv_seoul_team_schedule"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:text="경기 일정"
        android:textSize="20sp"
        android:fontFamily="@font/font_nanumsquare_bold"
        android:layout_marginStart="30dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@id/tv_seoul_team_real"
        app:layout_constraintStart_toStartOf="parent"
        />

    <ImageView
        android:id="@+id/line1"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        app:layout_constraintTop_toBottomOf="@id/tv_seoul_team_schedule"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        android:src="@drawable/bg_gray_line_thick"
        />


    <ImageView
        android:id="@+id/iv_seoul_team_schedule"
        android:layout_width="350dp"
        android:layout_height="90dp"
        android:layout_marginTop="7dp"
        android:src="@drawable/bg_slide_bar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/line1"
        />

    <ImageView
        android:id="@+id/iv_seoul_team_schedule2"
        android:layout_width="340dp"
        android:layout_height="70dp"
        android:layout_marginTop="10dp"

        android:src="@drawable/img_match_schedule"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/iv_seoul_team_schedule"
        />






    <TextView
        android:id="@+id/tv_seoul_team_players"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"

        android:text="선수단"
        android:textSize="20sp"
        android:fontFamily="@font/font_nanumsquare_bold"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/iv_seoul_team_schedule"
        app:layout_constraintStart_toStartOf="parent"
        />

    <ImageView
        android:id="@+id/line2"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        app:layout_constraintTop_toBottomOf="@id/tv_seoul_team_players"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        android:src="@drawable/bg_gray_line_thick"
        />



    <ImageView
        android:id="@+id/iv_seoul_player"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:src="@drawable/img_player_list"
        android:layout_marginTop="7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/line2"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <ImageView
        android:id="@+id/iv_seoul_player2"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:src="@drawable/img_player_list"
        android:layout_marginTop="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_seoul_player"
        app:layout_constraintEnd_toEndOf="parent"
        />


    <TextView
        android:id="@+id/tv_seoul_team_recent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"

        android:text="구단 소식"
        android:textSize="20sp"
        android:fontFamily="@font/font_nanumsquare_bold"
        android:layout_marginStart="30dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@id/iv_seoul_player2"
        app:layout_constraintStart_toStartOf="parent"
        />

    <ImageView
        android:id="@+id/line3"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        app:layout_constraintTop_toBottomOf="@id/tv_seoul_team_recent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        android:src="@drawable/bg_gray_line_thick"
        />

    <TextView
        android:id="@+id/tv_seoul_recent_news1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="‘고승범 가격’ FC서울 코치, 3경기 출장 정지"
        app:layout_constraintTop_toBottomOf="@id/line3"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="40dp"
        />

    <TextView
        android:id="@+id/tv_seoul_recent_news2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="'AG 금메달' 조영욱, 약 10개월간 군 생활 마치고 FC 서울..."
        app:layout_constraintTop_toBottomOf="@id/tv_seoul_recent_news1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="40dp"
        />

    <TextView
        android:id="@+id/tv_seoul_recent_news3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="프로축구 FC서울, 창단 40주년 기념 자선 경매"
        app:layout_constraintTop_toBottomOf="@id/tv_seoul_recent_news2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="40dp"
        />



</androidx.constraintlayout.widget.ConstraintLayout>